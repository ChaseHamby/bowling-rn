{
  "expo": {
    "name": "Bowling",
    "slug": "Bowling",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.cortezashley.bowling",
      "supportsTablet": true
    },
    "android": {
      "package": "com.cortezashley.bowling"
    },
    "description": "Bowling Score Keeper",
    "githubUrl": "https://github.com/redbean87/bowling-rn.git"
  }
}
